  "description": "Pet's are people too!",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "build": "ng build -prod -nc -oh=media",
    "start": "node src/server/server.js",
    "dev": "concurrently \"ng build --prod -oh=media -w\" \"node src/server/server.js\"",
    "devMongo": "concurrently \"mongod\" \"ng build -w\" \"node src/server/server.js\"",
    "deploy-prod": "concurrently \"npm run bitbucket\" \"npm run heroku\"",
    "deploy-dash": "concurrently \"npm run bitbucket-dev\" \"npm run heroku-dash\"",
    "postinstall": "npm run build",
    "bitbucket": "git push bitbucket master",
    "bitbucket-dev": "git push bitbucket dev",
    "heroku": "git push heroku-production master",
    "heroku-dash": "git push heroku-dashboard dev:master",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
